{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder":"swc",
    "typeCheck": true,
    "assets": [
      "templates/**/*",
      "core/database/default-data.json"
    ]
  }
}
